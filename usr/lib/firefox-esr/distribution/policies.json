{
    "policies": {
        "DisableAppUpdate": true,
        "AppUpdateURL": "https://update.invalid", 
        "NoDefaultBookmarks": true,
        "DefaultDownloadDirectory": "${home}/Downloads",
        "DisableTelemetry": true,
        "DisableFirefoxStudies": true,
        "DisablePocket": true,
        "NetworkPrediction": false,
        "SearchBar": "separate",
        "SearchSuggestEnabled": false,
        "Cookies": {
            "Default": true,
            "AcceptThirdParty": "from-visited",
            "ExpireAtSessionEnd": true,
            "RejectTracker": true,
            "Locked": false
        },
        "EnableTrackingProtection": {
            "Value": true,
            "Locked": false
        },
        "SearchEngines": {
            "Default": "DuckDuckGo",
            "Add": [
                {
                "Name": "Ecosia",
                "URLTemplate": "https://www.ecosia.org/search?q={searchTerms}",
                "Method": "GET",
                "IconURL": "https://cdn-static.ecosia.org/static/icons/favicon.ico",
                "Alias": "@ecosia",
                "Description": "Ecosia Search"
                },
                {
                "Name": "Qwant",
                "URLTemplate": "https://qwant.com/?q={searchTerms}&hc=0&hti=0&vt=1&b=1&si=0&c=blue&ch=none&eco_suggest=0&t=web",
                "Method": "GET",
                "IconURL": "https://qwant.com/favicon.ico",
                "Alias": "@qwant",
                "Description": "Qwant"
                },
                {
                "Name": "Startpage",
                "URLTemplate": "https://www.startpage.com/sp/search?query={searchTerms}&cat=web&pl=opensearch",
                "Method": "GET",
                "IconURL": "https://www.startpage.com/favicon.ico",
                "Alias": "@startpage",
                "Description": "Startpage"
                },
                {
                "Name": "MetaGer",
                "URLTemplate": "https://metager.org/meta/meta.ger3?focus=web&eingabe={searchTerms}&encoding=utf8&lang=en",
                "Method": "GET",
                "IconURL": "https://metager.org/favicon.ico",
                "Alias": "@metager",
                "Description": "MetaGer"
                }
            ]
        },
        "PopupBlocking": {
            "Default": true,
            "Locked": false
        },
        "FirefoxHome": {
            "Search": true,
            "Topsites": true,
            "Highlights": false,
            "Pocket": false,
            "Snippets": false,
            "Locked": false
        },
        "SupportMenu": {
            "Title": "SolydXK Support",
            "URL": "https://forums.solydxk.com",
            "AccessKey": "S"
        },
        "DNSOverHTTPS": {
            "Enabled": false,
            "ProviderURL": " ",
            "Locked": false
        }
    }
}
